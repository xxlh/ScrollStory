    "@pixi/core": "^5.1.3",
    "@pixi/display": "^5.1.3",
    "@pixi/graphics": "^5.1.3",
    "@pixi/interaction": "^5.1.3",
    "@pixi/loaders": "^5.1.3",
    "@pixi/math": "^5.1.0",
    "@pixi/runner": "^5.1.1",
    "@pixi/settings": "^5.1.3",
    "@pixi/sprite": "^5.1.3",
    "@pixi/sprite-animated": "^5.1.3",
    "@pixi/spritesheet": "^5.1.3",
    "@pixi/text": "^5.1.3",
    "@pixi/ticker": "^5.1.3",
    "@pixi/utils": "^5.1.3",
